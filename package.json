{
  "name": "platformsh-client",
  "version": "0.1.112",
  "description": "Isomorphic Javascript library for accessing the Platform.sh API",
  "browser": "lib/client/platform-api.js",
  "main": "lib/server/platform-api.js",
  "scripts": {
    "build": "rm -rf ./lib && webpack --env.mode=build",
    "dev": "webpack --progress --colors --watch --env.mode=dev",
    "test": "npm run test-web && npm run test-node",
    "test-web": "mocha --require babel-core/register -r mock-local-storage ./test/.setup.js ./test/**/*.spec.js",
    "test-node": "mocha --require babel-core/register -r ./test/.setup-node.js ./test/**/*.spec.js",
    "eslint": "eslint .",
    "precommit": "lint-staged && npm run build",
    "prepush": "npm test"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.16.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.16.3",
    "chai": "3.5.0",
    "eslint": "^4.18.2",
    "eslint-loader": "1.6.3",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-prettier": "^2.7.0",
    "fake-fetch": "^1.0.0",
    "fetch-mock": "^5.9.4",
    "husky": "^0.14.3",
    "jsdom": "^9.6.0",
    "lint-staged": "^7.3.0",
    "mocha": "^6.0.2",
    "mock-local-storage": "^1.1.8",
    "prettier": "1.13.4",
    "sinon": "^1.17.6",
    "webpack": "3.5.6",
    "webpack-merge": "^4.2.1",
    "yargs": "6.6.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "atob": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "basename": "^0.1.2",
    "detect-node": "^2.0.4",
    "email-validator": "^1.0.7",
    "es6-promise": "4.2.6",
    "is-scalar": "^1.0.2",
    "is-url": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "object.pick": "^1.2.0",
    "parse_url": "^0.1.1",
    "slugify": "^1.3.4",
    "to-querystring": "^1.1.0",
    "url-pattern": "^1.0.3"
  }
}
